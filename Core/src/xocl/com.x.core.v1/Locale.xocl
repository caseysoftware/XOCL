package com.x.core.v1
{
	record Locale
	{
		summary "Specifies fields for the locale."
		description "Contains fields that identify the locale, such as country, language, etc."
		
		string language "2-letter language code. For example, 'en' (English) or 'fr' (French).";

		string country "2-character country code. For example, US (United States) or FR (France).";

        string? variant "Variant for the locale.";
	}
}